{
    "name": "programster/pgsql-logger",
    "description": "A logger that logs to a PostgreSQL database and is compatible with PSR-3",
    "keywords": ["log", "logging", "pgsql"],
    "homepage": "https://github.com/programster/package-pgsql-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "homepage": "https://blog.programster.org"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "psr/log": "^3.0",
        "programster/log": "^2.0",
        "ext-pgsql": "*",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Programster\\PgsqlLogger\\": "src/"
        }
    }
}
